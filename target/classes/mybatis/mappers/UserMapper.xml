<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lanzhou.wanshou.dao.UserDao">
    <select id="findUserById" parameterType="java.lang.Integer"
            resultType="com.lanzhou.wanshou.entity.User">
      select t.id,t.name from user_info t where t.id = #{id}
    </select>
</mapper>